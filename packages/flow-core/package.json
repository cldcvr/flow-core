{
  "name": "@cldcvr/flow-core",
  "version": "1.26.4",
  "description": "Core package of flow design system",
  "module": "dist/flow-core.es.js",
  "main": "dist/flow-core.cjs.js",
  "types": "dist/src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "./compile.sh",
    "compile": "./compile.sh",
    "meta-build": "./meta-build.sh",
    "build": "vite build --emptyOutDir && vite build --emptyOutDir --config vite.umd.config.ts && tsc",
    "build:watch": "concurrently --kill-others \"vite build --emptyOutDir --watch\" \"tsc --watch\"",
    "sync-colors": "node ./figma/sync-colors.js",
    "sync-md-file": "node ./figma/sync-md-file.js",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\" && wca analyze src --format vscode --outFile html.html-data.json",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "test-build": "web-test-runner ./src/**/*.test.ts --node-resolve",
    "test": "pnpm run build && web-test-runner ./src/**/*.test.ts --node-resolve",
    "test:file": "pnpm run build && web-test-runner --node-resolve",
    "test:watch": "pnpm run build && web-test-runner ./src/**/*.test.ts --node-resolve --watch"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "dependencies": {
    "@cldcvr/flow-core-config": "workspace:*",
    "@emoji-mart/data": "^1.1.2",
    "@floating-ui/dom": "^1.0.2",
    "axios": "^0.27.2",
    "emoji-mart": "^5.5.2",
    "flatpickr": "^4.6.13",
    "lit": "^2.2.4",
    "lodash-es": "^4.17.21",
    "rxjs": "^7.5.7",
    "validate-color": "^2.2.1"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.8.1",
    "@open-wc/testing": "^3.1.5",
    "@types/jest": "29.5.5",
    "@types/lodash-es": "^4.17.6",
    "@web/dev-server-esbuild": "^0.4.1",
    "@web/test-runner": "^0.17.1",
    "concurrently": "^8.2.1",
    "esbuild-sass-plugin": "2.2.6",
    "fs-extra": "^11.1.1",
    "lit-html": "^2.2.5",
    "sass": "^1.52.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vue": "2.6.14",
    "web-component-analyzer": "^2.0.0-next.4"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/cldcvr/flow-core.git",
    "directory": "packages/flow-core"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "customElements": "custom-elements.json",
  "bugs": {
    "url": "https://github.com/cldcvr/flow-core/issues"
  },
  "homepage": "https://github.com/cldcvr/flow-core#readme",
  "author": "@cldcvr",
  "license": "MIT"
}
